@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.Products>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "Master.cshtml";
	foreach (var keyword in @Model.PageKeywords){
ViewData["PageKeywords"] +=$"{keyword},";
}
}

<div class="container">
    <div class="products row row-cols-1 row-cols-xxl-4 row-cols-lg-3 row-cols-md-2 justify-content-between">
        <partial name="ProductsPartial"/>
    </div>
</div>